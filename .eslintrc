{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": 0,
    "require-jsdoc": 0,
    "arrow-parens": 0,
    "object-curly-spacing": 0,
    "comma-dangle": 0,
    "max-len": ["warn", { "ignoreUrls": true, "ignoreComments": true, "code": 150 }],
    "new-cap": ["warn", { "properties": false }]
  },
  "extends": [
    "eslint:recommended",
    "google"
  ]
}